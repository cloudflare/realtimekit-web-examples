{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "examples",
  "main": "./build/server/index.js",
  "assets": {
    "directory": "./build/client"
  },
  "compatibility_date": "2024-01-01",
  "observability": {
    "enabled": false,
    "head_sampling_rate": 1,
    "logs": {
      "enabled": true,
      "head_sampling_rate": 1,
      "persist": true,
      "invocation_logs": true
    },
    "traces": {
      "enabled": false,
      "persist": true,
      "head_sampling_rate": 1
    }
  }
}
